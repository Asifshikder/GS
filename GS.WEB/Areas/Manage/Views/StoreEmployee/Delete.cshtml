@model GS.DATA.Entities.Shop.StoreEmployee
@{
    ViewData["Title"] = "Delete";
}
<div class="breadcrumbs">
    <div class="breadcrumbs-inner">
        <div class="row m-0">
            <div class="col-sm-4">
                <div class="page-header float-left">
                    <div class="page-title">
                        <h1>Delete</h1>
                    </div>
                </div>
            </div>
            <div class="col-sm-8">
                <div class="page-header float-right">
                    <div class="page-title">
                        <ol class="breadcrumb text-right">
                            <li><a>StoreEmployee</a></li>
                            <li><a href="/Manage/StoreEmployee/Index">Index</a></li>
                            <li><a>Delete</a></li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="content">
    <div class="animated fadeIn">
        <div class="card">
            <div class="row">

                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title">Delete StoreEmployee</strong>
                        </div>
                        <div class="card-body">
                            <form asp-area="Manage" asp-controller="StoreEmployee" asp-action="DeleteConfirm" method="post">
                                <div class="form-group">
                                    <label class=" form-control-label">Name</label>
                                    <input type="text" asp-for="FullName" class="form-control" disabled>
                                </div>
                                @Html.HiddenFor(model => model.Id)
                                @Html.HiddenFor(model => model.Designation)
                                @Html.HiddenFor(model => model.Email)
                                @Html.HiddenFor(model => model.StoreId)
                                @Html.HiddenFor(model => model.PhoneNumber)
                                <div class="form-group">
                                    <button type="submit" class="btn btn-danger">Delete</button>
                                    <a href="/Manage/StoreEmployee/Index" class="btn btn-warning">Cancel</a>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>